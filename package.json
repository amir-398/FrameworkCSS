{
  "name": "anniv",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "npm-run-all --parallel header card leftComponent rightComponent home birthdayEllipse loadingBarComponent ",
    "home": "sass --watch ./src/pages/home.scss ./src/pages/home.css",
    "header": "sass --watch ./src/components/header/header.scss ./src/components/header/header.css",
    "card": "sass --watch ./src/components/cardComponent/card.scss ./src/components/cardComponent/card.css",
    "leftComponent": "sass --watch ./src/components/leftComponent/leftComponent.scss ./src/components/leftComponent/leftComponent.css",
    "rightComponent": "sass --watch ./src/components/rightComponent/rightComponent.scss ./src/components/rightComponent/rightComponent.css",
    "birthdayEllipse": "sass --watch ./src/components/birthdayEllipse/birthdayEllipse.scss ./src/components/birthdayEllipse/birthdayEllipse.css",
    "loadingBarComponent": "sass --watch ./src/components/loadingBarComponent/loadingBarComponent.scss ./src/components/loadingBarComponent/loadingBarComponent.css"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "npm-run-all": "^4.1.5",
    "vite": "^5.1.0"
  }
}
